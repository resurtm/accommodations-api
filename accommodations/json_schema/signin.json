{
  "$schema": "http://json-schema.org/schema#",
  "description": "Validates data for sign in",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "minLength": 5,
      "maxLength": 50,
      "format": "email"
    },
    "password": {
      "type": "string",
      "minLength": 6,
      "maxLength": 255
    }
  },
  "minProperties": 2,
  "maxProperties": 2,
  "required": [
    "email",
    "password"
  ]
}
